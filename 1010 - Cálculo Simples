# Entrada dos dados da peça 1
codigo_peca1, numero_peca1, valor_unitario1 = map(float, input().split())

# Entrada dos dados da peça 2
codigo_peca2, numero_peca2, valor_unitario2 = map(float, input().split())

# Calculando o total a pagar
total_a_pagar = (numero_peca1 * valor_unitario1) + (numero_peca2 * valor_unitario2)

# Imprimindo o resultado
print("VALOR A PAGAR: R$ {:.2f}".format(total_a_pagar))
