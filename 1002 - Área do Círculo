using System;

class MainClass {
    public static void Main (string[] args) {
        double raio, area, pi = 3.14159;

        // Recebendo o valor do raio
        raio = Convert.ToDouble(Console.ReadLine());

        // Calculando a Ã¡rea
        area = pi * raio * raio;

        // Apresentando o resultado
        Console.WriteLine($"A={area:F4}");
    }
}
